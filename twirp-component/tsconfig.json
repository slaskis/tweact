{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "typeRoots": ["./node_modules/@types", "./types"],
    "lib": ["dom", "es2015", "es2016"],
    "strict": true,
    "downlevelIteration": true
  }
}
